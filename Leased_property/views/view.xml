<?xml version='1.0' encoding='utf-8'?>
<odoo>
  
  <record id="leas_property_tree_view" model="ir.ui.view">
    <field name="name">Leas property </field>
    <field name="model">leased.property</field>
    <field name="arch" type="xml">
      <tree>
         <field name="name"/> 
          <field name="partner_id"/>
       <!-- <field name="lease_duration"/> -->
      </tree>
    </field>
  </record>

  <record id="leas_form_view" model="ir.ui.view">
    <field name="name">Leas property </field>
    <field name="model">leased.property</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
        <group>
           <field name="name"/> 
            <field name="partner_id"/>
        </group>    
        </sheet>
      </form>
    </field>
  </record>

  <record id="leas_property_tree" model="ir.ui.view">
    <field name="name">Leas property </field>
    <field name="model">leas.propertys</field>
    <field name="arch" type="xml">
      <tree>
        <!-- <field name="name"/> -->
        <field name="partner_id"/>
         <field name="property_type_id"/>
        <field name="lease_duration"/>
      </tree>
    </field>
  </record>

  <record id="leas_form" model="ir.ui.view">
    <field name="name">Leas property </field>
    <field name="model">leas.propertys</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
        <group>
          <!-- <field name="name"/> -->
          <field name="lease_duration"/>
           <field name="start_Date"/>
          <field name="date_deadline"/>
          <field name="partner_id"/>
         <field name="property_type_id"/>
          <!-- <field name="validity"/> -->
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>